<!-- main header ends-->
        <section id="about-banner" class="about-banner">
                <div class="container">
                <div class="about-banner-wrapper full-height">
                   <div class="about-heading">
                        <h3 class="wow fadeInUp" data-wow-delay="0.3s">Hey ! Salut</h3>
                        <h1 class="wow fadeInUp" data-wow-delay="0.6s">
                            Nous sommes heureux de vous voir <br> apprendre plus de nous.
                        </h1> 
                    </div>     
                </div>
                </div>

        </section>

        <section id="about-us" class="about-us page">
            <div class="container wow fadeInUp" data-wow-delay="0.3s">
                <div class="row">
                {% for item in about %}
                    <div class="about-wrapper">
                        <div class="col-md-6">
                            <h1>A propos de nous ..</h1>
                            <div class="section-snippet">
                              <span class="snippet-heading">Etablie le</span>
                              <span>{{item.eshtablished}}</span>  
                            </div>
                            <div class="section-snippet">
                              <span class="snippet-heading">Membre de l'équipe</span>
                              <span>{{item.members}} </span>  
                            </div>
                        </div>
                        <div class="col-md-6">
                            <p>{{item.presentation | raw}} </p>
                        </div>
                    </div>
                {% endfor %}
                    
                </div>
            </div>
        </section>

        <section id="team-members" class="team-members page">
            <div class="container">
                <div class="row">
                    <div class="team-member clearfix wow fadeInUp" data-wow-delay="0.3s">
                    {% for item in team %}
                        <div class="col-md-6">
                            <!-- <div class="team-member-img center-block"> -->
                                <img src="images/{{item.image}}" alt="" class="team-member-img center-block">
                            <!-- </div> -->
                            <table class="team-member-contact center-block">
                                <tbody>
                                    <tr>
                                        <td>E-mail:</td>
                                        <td>{{item.email}}</td>
                                    </tr>
                                    <tr>
                                        <td>Téléphone:</td>
                                        <td>+225 {{item.phone}} </td>
                                    </tr>
                                    <tr>
                                        <td>Facebook:</td>
                                        <td>{{item.facebook}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="col-md-6">
                            <div class="team-member-desc">
                                <div class="team-member-desc-heading">
                                    <h4>{{item.name}}</h4>
                                    <span>{{item.post}}</span>
                                </div>
                                <p>
                                    {{item.competence | raw}}
                                </p>
                            </div>
                        </div>
                    {% endfor %}
                    </div>
                </div>                                   
            </div>
        </section>
<!-- CTA section starts -->
        <section id="cta" class="cta page">
            <div class="container wow fadeInUp">
                <div class="row">
                    <div class="col-sm-6">
                        <h1>Laissez nous vous aider ..</h1>
                    </div>
                    <div class="col-sm-6">
                        <button class="btn btn-block custom-button">Contactez-nous</button>
                    </div>
                </div><!-- End: .row -->
            </div><!-- End: .container-->
        </section>
        <!-- CTA section ends -->